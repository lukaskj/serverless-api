{
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf .build .serverless .webpackCache",
    "build": "yarn lint && tsc -p .",
    "offline": "yarn clean && serverless offline",
    "sls:package": "yarn clean && sls package",
    "sls:deploy": "yarn clean && serverless deploy",
    "sls:remove": "serverless remove",
    "sls:down": "serverless remove",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest tests",
    "test:watch": "jest tests --runInBand --collectCoverage false --watch",
    "local": "ts-node src/localServer.ts",
    "typeorm": "cross-env NODE_ENV=migration ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "yarn run typeorm migration:generate -n",
    "typeorm:run": "yarn run typeorm migration:run",
    "typeorm:revert": "yarn run typeorm migration:revert"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-typescript": "^7.14.5",
    "@types/body-parser": "^1.19.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.3.1",
    "@typescript-eslint/eslint-plugin": "^4.28.2",
    "@typescript-eslint/parser": "^4.28.2",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "copy-webpack-plugin": "^9.0.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-sonarjs": "^0.9.1",
    "fork-ts-checker-webpack-plugin": "^6.2.12",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "serverless": "^2.51.2",
    "serverless-offline": "^7.0.0",
    "serverless-webpack": "^5.5.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "multer": "^1.4.2",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "serverless-http": "^2.7.0",
    "typedi": "^0.10.0"
  }
}
