{
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "clean": "rm -rf .build .serverless && mkdir .build",
    "copy": "cp .env .build/",
    "build": "yarn lint && tsc -p . && yarn copy",
    "offline": "yarn clean && yarn copy && serverless offline",
    "deploy": "yarn clean && yarn copy && serverless deploy",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "test": "jest tests",
    "test:watch": "jest tests --runInBand --collectCoverage false --watch",
    "local": "ts-node src/localServer.ts",
    "remove": "serverless remove",
    "down": "serverless remove",
    "typeorm": "cross-env NODE_ENV=migration ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "yarn run typeorm migration:generate -n",
    "typeorm:run": "yarn run typeorm migration:run",
    "typeorm:revert": "yarn run typeorm migration:revert"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.7.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.1",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-sonarjs": "^0.10.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "serverless": "^2.55.0",
    "serverless-offline": "^8.0.0",
    "serverless-plugin-typescript": "^1.1.9",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "multer": "^1.4.3",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.9.0",
    "serverless-http": "^2.7.0",
    "sqlite3": "^5.0.2",
    "typedi": "^0.10.0",
    "typeorm": "^0.2.37"
  }
}
